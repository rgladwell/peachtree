# should generate default index html file
> lanyon:generate-site
$ exists target/lanyon/site/
$ exists target/lanyon/site/index.html
$ exists src/main/lanyon/pages
$ exists src/main/lanyon/pages/index.page
> verify-index-html-title
> verify-index-html-h1

# should create new pages
> lanyon:add-page about
$ exists src/main/lanyon/pages/about.page
> lanyon:generate-site
$ exists target/lanyon/site/about.html
