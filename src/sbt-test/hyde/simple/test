# should generate default index html file
> hyde:generate-site
$ exists target/hyde/site/
$ exists target/hyde/site/index.html
$ exists src/main/hyde/pages
$ exists src/main/hyde/pages/index.page
> verify-index-html-title
> verify-index-html-h1

# should create new pages
> hyde:add-page about
$ exists src/main/hyde/pages/about.page
> hyde:generate-site
$ exists target/hyde/site/about.html
